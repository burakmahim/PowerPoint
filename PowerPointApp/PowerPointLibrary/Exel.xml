	ÇALIŞAN FORMULLER VE GRAFİK

	<excel>
		<sheet name="Hesaplama">
			<table name="Hesaplama" startCell="A1">
				<row>
					
					<cell bold="true">Değer 1</cell>
					<cell bold="true">Değer 2</cell>
					<cell bold="true">Toplam</cell>
					<cell bold="true">Çarpım</cell>
					<cell bold="true">Fark</cell>
					<cell bold="true">Bölüm</cell>
				</row>
				<row>
					<cell>5</cell>
					<cell>10</cell>
					<cell formulaByOperation="toplama" applyTo="row" startIndex="0" endIndex="1"/>
					<cell formulaByOperation="çarpma" applyTo="row" startIndex="0" endIndex="1"/>
					<cell formulaByOperation="çıkartma" applyTo="row" startIndex="1" endIndex="0"/>
					<cell formulaByOperation="bölme" applyTo="row" startIndex="1" endIndex="0"/>
				</row>
				<row>
					<cell>12</cell>
					<cell>4</cell>
					<cell formulaByOperation="toplama" applyTo="row" startIndex="0" endIndex="1"/>
					<cell formulaByOperation="çarpma" applyTo="row" startIndex="0" endIndex="1"/>
					<cell formulaByOperation="çıkartma" applyTo="row" startIndex="0" endIndex="1"/>
					<cell formulaByOperation="bölme" applyTo="row" startIndex="0" endIndex="1"/>
				</row>
			</table>

			<chart type="Column" title="Hesaplama Sonuçları" xAxis="İşlem" yAxis="Değer" sourceTable="Hesaplama"/>
		</sheet>
	</excel>



	formül

	<excel>
		<sheet>
			<table name="Hesaplama">
				<row>
					<cell bold="true">Açıklama</cell>
					<cell bold="true">Değer 1</cell>
					<cell bold="true">Değer 2</cell>
					<cell bold="true">Sonuç</cell>
				</row>

				<row>
					<cell>Toplama</cell>
					<cell>10</cell>
					<cell>20</cell>
					<cell formula="=B2+C2"></cell>
				</row>
				<row>
					<cell>Çıkarma</cell>
					<cell>50</cell>
					<cell>30</cell>
					<cell formula="=B3-C3"></cell>
				</row>

				<row>
					<cell>Çarpma</cell>
					<cell>5</cell>
					<cell>4</cell>
					<cell formula="=B4*C4"></cell>
				</row>

				<row>
					<cell>Bölme</cell>
					<cell>100</cell>
					<cell>25</cell>
					<cell formula="=B5/C5"></cell>
				</row>
				<row>
					<cell>Ortalama</cell>
					<cell>80</cell>
					<cell>100</cell>
					<cell formula="=AVERAGE(B6:C6)"></cell>
				</row>

				<row>
					<cell>En Küçük</cell>
					<cell>15</cell>
					<cell>5</cell>
					<cell formula="=MIN(B7:C7)"></cell>
				</row>

				<row>
					<cell>En Büyük</cell>
					<cell>33</cell>
					<cell>77</cell>
					<cell formula="=MAX(B8:C8)"></cell>
				</row>

				<row>
					<cell>Geçti mi?</cell>
					<cell>60</cell>
					<cell></cell>
					<cell formula="=IF(B9&gt;=50, &quot;Geçti&quot;, &quot;Kaldı&quot;)"></cell>
				</row>
				<row>
					<cell>Yuvarlama</cell>
					<cell>3,14159</cell>
					<cell></cell>
					<cell formula="=ROUND(B10; 2)"></cell>
				</row>

				<row>
					<cell>Mod (kalan)</cell>
					<cell>17</cell>
					<cell>4</cell>
					<cell formula="=MOD(B11; C11)"></cell>
				</row>

				<row>
					<cell>Metin uzunluğu</cell>
					<cell>Merhaba</cell>
					<cell></cell>
					<cell formula="=LEN(B12)"></cell>
				</row>

				<row>
					<cell>Ad + Soyad</cell>
					<cell>Ali</cell>
					<cell>Veli</cell>
					<cell formula="=B13 &amp; &quot; &quot; &amp; C13"></cell>
				</row>

				<row>
					<cell>Bugünün Tarihi</cell>
					<cell></cell>
					<cell></cell>
					<cell formula="=TODAY()"></cell>
				</row>

				<row>
					<cell>Şu An</cell>
					<cell></cell>
					<cell></cell>
					<cell formula="=NOW()"></cell>
				</row>

				<row>
					<cell>Hata varsa mesaj</cell>
					<cell>0</cell>
					<cell></cell>
					<cell formula="=IFERROR(10/B17; &quot;Hata&quot;)"></cell>
				</row>
			</table>

			<chart type="Column" title="Hesaplama Sonuçları" xAxis="İşlem" yAxis="Değer" sourceTable="Hesaplama"/>

		</sheet>
	</excel>
	

</excels>
	